{
  "manifest_version": 2,
  "name": "Test Extension",
  "version": "0.0",
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["inject.js"],
    "all_frames": true
  }],
  "web_accessible_resources": [
    "content.js"
  ],
  "browser_action": {
    "default_title": "Test",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ]
}
